{
    "name": "@runkai/eslint-config",
    "type": "module",
    "version": "1.6.4",
    "packageManager": "pnpm@8.6.0",
    "description": "Svelte ESLint config",
    "author": "Runkai Zhang (https://github.com/runkaiz/)",
    "license": "MIT",
    "homepage": "https://github.com/runkaiz/eslint-config#readme",
    "repository": "runkaiz/eslint-config",
    "keywords": [
        "eslint",
        "config",
        "eslint-config",
        "svelte",
        "prettier"
    ],
    "main": "index.js",
    "scripts": {
        "lint": "eslint && prettier . -c",
        "lint:debug": "eslint --debug",
        "lint:fix": "eslint --fix",
        "lint:ci": "eslint --format junit -o reports/junit/eslint-results.xml",
        "format": "prettier . --write",
        "release": "changeset publish",
        "inspect": "npx @eslint/config-inspector"
    },
    "peerDependencies": {
        "@prettier/plugin-xml": "^3.4.1",
        "eslint": "^9.2.0",
        "prettier": "3.2.5",
        "svelte": "^4.2.16"
    },
    "dependencies": {
        "@antfu/eslint-config": "^2.27.3",
        "@eslint/eslintrc": "^3.3.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-format": "^0.1.3",
        "eslint-plugin-svelte": "2.36.0-next.13",
        "eslint-plugin-tailwindcss": "^3.18.0",
        "prettier-plugin-svelte": "^3.4.0",
        "svelte-eslint-parser": "^0.35.0"
    },
    "devDependencies": {
        "@changesets/cli": "^2.29.4",
        "@eslint/eslintrc": "^3.0.2",
        "@svitejs/changesets-changelog-github-compact": "^1.2.0",
        "eslint": "^9.27.0",
        "prettier": "3.2.5"
    }
}
